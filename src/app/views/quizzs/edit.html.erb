<% provide(:title, 'Edit Quizz') %>
<h1>Edit Quizz</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@quizz) do |f| %>

      <%= f.label :name %>
      <%= f.text_field :name, class: 'form-control' %>

      <%= f.submit "Update quizz", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
